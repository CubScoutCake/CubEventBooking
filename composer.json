{   
    "name": "JacobAGTyler/CubsBooking",
    "description": "An app to manage booking for Cub Scouts by leaders.",
    "homepage": "https://github.com/CubScoutCake/CubEventBooking",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "@stable",
        "cakephp/cakephp": "@stable",
        "mobiledetect/mobiledetectlib": "@stable",
        "cakephp/migrations": "@stable",
        "cakephp/plugin-installer": "@stable",
        "dompdf/dompdf": "^0.8.2",
        "friendsofcake/cakepdf": "@stable",
        "tecnickcom/tcpdf" : "@stable",
        "mpdf/mpdf" : "@stable",
        "friendsofcake/bootstrap-ui": "@stable",
        "friendsofcake/cakephp-csvview": "@stable",
        "muffin/trash": "dev-master",
        "friendsofcake/search": "@stable",
        "robmorgan/phinx": "@stable",
        "dereuromark/cakephp-databaselog": "@stable",
        "josegonzalez/cakephp-upload": "^3.6",
        "riesenia/cakephp-duplicatable": "^3.0"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "cakephp/debug_kit": "@stable",
        "cakephp/cakephp-codesniffer": "@stable",
        "cakephp/bake": "@stable",
        "codeclimate/php-test-reporter": "@stable",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump",
        "cs-check": "vendor/bin/phpcs -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./tests ./src/Model ./src/View ./src/Network ./src/Shell ./src/Form ./src/Console ./src/Mailer ./src/Controller/Register ./src/Controller/Component ./src/Controller/Api",
        "cs-fix": "vendor/bin/phpcbf -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./tests ./src",
        "test": "phpunit --colors=always"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
