{   
    "name": "JacobAGTyler/CubsBooking",
    "description": "An app to manage booking for Cub Scouts by leaders.",
    "homepage": "http://hertscubs.uk",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "cakephp/cakephp": "@stable",
        "mobiledetect/mobiledetectlib": "2.*",
        "cakephp/migrations": "*",
        "cakephp/plugin-installer": "*",
        "dompdf/dompdf": "*",
        "friendsofcake/cakepdf": "3.*",
        "tecnickcom/tcpdf" : "@stable",
        "mpdf/mpdf" : "^6.1",
        "friendsofcake/bootstrap-ui": "*",
        "fheider/cakephp-datatables": "dev-master",
        "friendsofcake/cakephp-csvview": "*",
        "muffin/trash": "dev-master",
        "friendsofcake/CakePdf": "*",
        "friendsofcake/search": "^1.2",
        "dereuromark/cakephp-ajax": "dev-master",
        "syntaxera/cakephp-sparkpost-plugin": "^0.4.5",
        "php-http/httplug": "@stable",
        "php-http/client-common": "@stable",
        "php-http/socket-client": "@stable",
        "SparkPost/php-sparkpost": "*",
        "SparkPost/SparkPost": "*"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "cakephp/debug_kit": "~3.0",
        "cakephp/cakephp-codesniffer": "2.*",
        "phpunit/phpunit": "^5.4",
        "cakephp/bake": "^1.2",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
