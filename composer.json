{   
    "name": "JacobAGTyler/CubsBooking",
    "description": "An app to manage booking for Cub Scouts by leaders.",
    "homepage": "https://github.com/CubScoutCake/CubEventBooking",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "@stable",
        "cakephp/cakephp": "@stable",
        "mobiledetect/mobiledetectlib": "@stable",
        "cakephp/migrations": "@stable",
        "cakephp/plugin-installer": "@stable",
        "dompdf/dompdf": "@stable",
        "friendsofcake/cakepdf": "@stable",
        "tecnickcom/tcpdf" : "@stable",
        "mpdf/mpdf" : "@stable",
        "friendsofcake/bootstrap-ui": "@stable",
        "friendsofcake/cakephp-csvview": "@stable",
        "muffin/trash": "dev-master",
        "friendsofcake/search": "@stable",
        "syntaxera/cakephp-sparkpost-plugin": "dev-develop",
        "php-http/httplug": "@stable",
        "php-http/client-common": "@stable",
        "php-http/socket-client": "@stable",
        "robmorgan/phinx": "@stable",
        "dereuromark/cakephp-databaselog": "@stable"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "cakephp/debug_kit": "@stable",
        "cakephp/cakephp-codesniffer": "@stable",
        "phpunit/phpunit": "@stable",
        "cakephp/bake": "@stable",
        "codeclimate/php-test-reporter": "@stable"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump",
        "cs-check": "vendor/bin/phpcs -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./tests ./src/Model ./src/View ./src/Network ./src/Shell ./src/Form ./src/Console ./src/Mailer",
        "cs-fix": "vendor/bin/phpcbf -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./tests ./src/Model ./src/View ./src/Network ./src/Shell ./src/Form ./src/Console ./src/Mailer"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
