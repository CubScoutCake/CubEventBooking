{   
    "name": "JacobAGTyler/CubsBooking",
    "description": "An app to manage booking for Cub Scouts by leaders.",
    "homepage": "https://github.com/CubScoutCake/CubEventBooking",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7",
        "cakephp/cakephp": "@stable",
        "mobiledetect/mobiledetectlib": "*",
        "cakephp/migrations": "@stable",
        "cakephp/plugin-installer": "*",
        "dompdf/dompdf": "*",
        "friendsofcake/cakepdf": "*",
        "tecnickcom/tcpdf" : "@stable",
        "mpdf/mpdf" : "^6.1",
        "friendsofcake/bootstrap-ui": "*",
        "friendsofcake/cakephp-csvview": "*",
        "muffin/trash": "dev-master",
        "friendsofcake/search": "^2.3",
        "syntaxera/cakephp-sparkpost-plugin": "dev-develop",
        "php-http/httplug": "@stable",
        "php-http/client-common": "@stable",
        "php-http/socket-client": "@stable",
        "robmorgan/phinx": "*",
        "dereuromark/cakephp-databaselog": "^2.2"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "cakephp/debug_kit": "^3.0",
        "cakephp/cakephp-codesniffer": "^2.0",
        "phpunit/phpunit": "^5.4",
        "cakephp/bake": "^1.2",
        "codeclimate/php-test-reporter": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump",
        "cs-check": "vendor/bin/phpcs -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./tests ./src/Model ./src/ ./src/Network ./src/Shell ./src/Form ./src/Console ./src/Mailer",
        "cs-fix": "vendor/bin/phpcbf -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./tests ./src/Model ./src/ ./src/Network ./src/Shell ./src/Form ./src/Console ./src/Mailer"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
